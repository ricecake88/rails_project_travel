<!-- edit a vacation -->
vacations/edit.html.erb

<%= render 'users/nav' %>
<%= render 'errors' %>
<%= form_for(@vacation) do |f| %>
    <label>Name:</label>
    <%= f.text_field :name %><br/>
    <%= f.date_select :start_date %><br/>
    <%= f.date_select :end_date %><br/>
    <%= f.submit "Edit Vacation Details"%>
<% end %>